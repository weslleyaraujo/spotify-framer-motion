{
  "name": "flashcards",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^0.1.0-beta.7",
    "@emotion/core": "^10.0.9",
    "@emotion/css": "^10.0.9",
    "@types/graphql": "^14.2.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "apollo-boost": "^0.4.0",
    "apollo-cache-inmemory": "^1.6.0",
    "concurrently": "^4.1.0",
    "emotion": "^10.0.9",
    "emotion-theming": "^10.0.9",
    "graphql": "^14.3.1",
    "react": "^16.8.5",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.5",
    "react-scripts": "3.0.1",
    "typescript": "^3.3.4000",
    "utility-types": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "graphql": "yarn graphql:create-introspection-json && concurrently --kill-others 'yarn graphql:create-devserver' 'yarn graphql:generate-types'",
    "graphql:create-devserver": "node ./scripts/create-graphql-dev-server.js",
    "graphql:create-introspection-json": "node ./scripts/create-instrospection-schema-json.js",
    "graphql:generate-types": "yarn apollo client:codegen --target typescript --outputFlat src/@graphql-types --localSchemaFile='./src/graphql/schema.json'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "apollo": "^2.12.4",
    "apollo-link-schema": "^1.2.2",
    "apollo-server": "^2.5.1",
    "graphql-tools": "^4.0.4"
  }
}
